{
    "name": "Scraper Vea 2",
    "description": "This extension will scrape the plaza vea website",
    "version": "1.1",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://www.plazavea.com.pe/*"],
        "js": ["content.js"]
      }
    ],
    "action":{
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "permissions": ["activeTab", "scripting"]
  }